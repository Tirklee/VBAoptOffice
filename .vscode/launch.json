{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "启动程序",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}\\js\\objInfo.js"
        },
        {
            "type": "node-terminal",
            "name": "运行脚本: test",
            "request": "launch",
            "command": "npm run test",
            "cwd": "${workspaceFolder}\\js"
        }
    ]
}